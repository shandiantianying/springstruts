<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE struts PUBLIC  
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"  
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.action.extension" value="do,action," />
	<package name="default" namespace="/" extends="struts-default">
		<action name="LoginAction" class="cn.shandian.tianying.User.LoginAction"
			method="execute">
			<result name="success">/show.jsp</result>
			<result name="input">/index.jsp</result>
		</action>
		<action name="registerAction" class="cn.shandian.tianying.User.LoginAction"
			method="register">
			<result name="success">/show.jsp</result>
			<result name="input">/index.jsp</result>
			<result name="register">/register.jsp</result>
		</action>
		
		        <action name="logout" class="cn.shandian.tianying.User.LoginAction" method="Logout">
            <result name="logout">/index.jsp</result>
        </action>
	</package>
</struts>
